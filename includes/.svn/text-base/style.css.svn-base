html,body {
	font-family: Georgia, "Times New Roman", Times, serif;
	font-size: 1em;
	background: transparent url('/img/backStripes.jpg') bottom repeat-x;
	text-align: center;
	padding: 0;
	margin: 0;
	height: 100%;
	min-height: 600px;
	}

img {
	border: 0;
	display: block;
}

a:link, a:visited {
	color:#0099FF;
}

a:hover, a:active; {
	color: #FF9900;
}


#TopFade {
	postion: absolute;
	top: 0;
	width: 100%;
	Height: 100px;
	Overflow: hidden;
	margin:0;
	background: transparent url('/img/WhiteFade.png') repeat-x;
}

#Brand {
	margin: auto;
	margin-top: 100px;
	
}